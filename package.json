  },
  "homepage": "https://github.com/ginetta/cockpit-sdk#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "es6-promise": "^4.2.8",
    "isomorphic-fetch": "^3.0.0",
    "query-string": "^6.13.7",
    "universal-websocket-client": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
